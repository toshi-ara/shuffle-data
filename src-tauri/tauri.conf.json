{
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devPath": "http://localhost:1420",
        "distDir": "../dist",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "shuffle-data",
        "version": "0.0.4"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "dialog": {
                "all": false,
                "open": true,
                "save": false,
                "message": true
            },
            "fs": {
                "scope": ["*"],
                "all": false,
                "readFile": false,
                "writeFile": false,
                "readDir": true,
                "copyFile": true,
                "createDir": false,
                "removeDir": false,
                "removeFile": false,
                "renameFile": false,
                "exists": true
            },
            "path": {
                "all": true
            },
            "process": {
                "all": true
            }
        },
        "bundle": {
            "active": false,
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "shuffle-data",
            "targets": "all"
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "Shuffle data",
                "width": 800,
                "height": 600
            }
        ]
    }
}
